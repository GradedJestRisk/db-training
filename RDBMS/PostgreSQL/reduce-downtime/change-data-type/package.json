{
   "name": "change-data-type",
   "main": "npm run type_change:start",
   "engines": {
      "node": "16.3.0",
      "npm": "7.15.1"
   },
   "scripts": {
      "database:start": "npm run database:start:instance && npm run database:start:wait-until-available",
      "database:start:instance": "docker-compose --file ./database/docker-compose.yml up --detach",
      "database:start:wait-until-available": "./database/wait-database-ready.sh",
      "fake_activity": "node src/fake-activity.js",
      "test_connexion": "node ./test-connexion.js",
      "type_change:perform": "node ./src/perform-type-change.js",
      "type_change:start": "run-p --race fake_activity type_change:perform"
   },
   "license": "ISC",
   "dependencies": {
      "pg": "^8.6.0"
   },
   "devDependencies": {
      "npm-run-all": "^4.1.5"
   }
}
